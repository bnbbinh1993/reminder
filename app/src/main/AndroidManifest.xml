<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reminder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowEventActivity"></activity>
        <activity android:name=".ui.work.ui.NoteActivity" />
        <activity android:name=".ui.work.ui.CountAgeActivity" />
        <activity android:name=".ui.work.ui.CountDayActivity" />
        <activity android:name=".ui.work.ui.CountTImeActivity" />
        <activity android:name=".ui.work.ui.StopwatchActivity" />
        <activity android:name=".ui.work.ui.TimerActivity" />
        <activity android:name=".ui.create.CreateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>